{
    "name": "mts7/benchmarks",
    "description": "PHP Benchmarks",
    "type": "library",
    "license": "MIT",
    "require": {
        "mts7/php-dependency-injection": "^1",
        "mts7/php-execution-timer": "^1"
    },
    "require-dev": {
        "infection/infection": "^0.26.15",
        "phing/phing": "^2.17",
        "phpmd/phpmd": "^2.13",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5",
        "roave/psalm-html-output": "^1.0",
        "roave/security-advisories": "dev-latest",
        "slevomat/coding-standard": "^8.5",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^4.27"
    },
    "autoload": {
        "psr-4": {
            "MtsBenchmarks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MtsBenchmarks\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "infection/extension-installer": true
        }
    }
}
