{
    "name": "mts7/benchmarks",
    "description": "PHP Benchmarks",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "mts7/php-dependency-injection": "^1",
        "mts7/php-execution-timer": "^1"
    },
    "require-dev": {
        "mts7/php-code-quality": "^0.1.17",
        "phpunit/phpunit": "^10.4",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "MtsBenchmarks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MtsBenchmarks\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "infection/extension-installer": true
        }
    }
}
